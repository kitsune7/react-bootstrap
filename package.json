{
  "name": "react-bootstrap",
  "description": "A bootstrap repository for getting a React.js project started up quickly",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "scripts": {
    "start": "npm run dev",
    "setup": "chmod +x scripts/* && scripts/setup.sh",
    "dev": "parcel src/index.html",
    "clean": "rm -rf dist .cache yarn.lock && yarn cache clean && yarn",
    "test": "jest --runInBand",
    "prebuild": "rm -rf dist",
    "build": "parcel build src/index.html",
    "format": "prettier --write .",
    "lint": "eslint src/**/*.tsx",
    "lint.fix": "eslint --fix src/**/*.tsx",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Christopher Bradshaw",
  "license": "MIT",
  "homepage": "https://github.com/kitsune7/react-bootstrap#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitsune7/react-bootstrap"
  },
  "bugs": {
    "url": "https://github.com/kitsune7/react-bootstrap/issues"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/react": "^11.0.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "core-js": "3.6.5",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "parcel": "^1.12.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
